{
  "name": "mysoutenance/app",
  "description": "Application soutenance PHP",
  "type": "project",
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "ext-mysqli": "*",
    "vlucas/phpdotenv": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "Backend\\": "src/Backend/",
      "Frontend\\": "src/Frontend/",
      "Service\\": "src/Service/",
      "Util\\": "src/Util/"
    }
  },
  "scripts": {
    "serve": "php -S localhost:8000 -t src/Public",
    "post-install-cmd": [
      "@composer dump-autoload"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}