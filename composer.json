{
  "name": "mysoutenance/app",
  "description": "Application soutenance PHP",
  "type": "project",
  "require": {
    "php": "^7.4",
    "ext-pdo": "*",
    "ext-mysqli": "*",
    "vlucas/phpdotenv": "^5.5",
    "nikic/fast-route": "^1.3",
    "phpmailer/phpmailer": "^6.5",
    "robthree/twofactorauth": "^1.8",
    "bacon/bacon-qr-code": "^2.0"
  },

  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\Config\\": "src/Config/",
      "App\\Backend\\Model\\": "src/Backend/Model/",
      "App\\Backend\\Service\\": "src/Backend/Service/",
      "App\\Backend\\Util\\": "src/Backend/Util/",
      "App\\Backend\\Controller\\": "src/Backend/Controller/",
      "App\\Backend\\Controller\\Administration\\": "src/Backend/Controller/Administration/",
      "App\\Backend\\Controller\\Commission\\": "src/Backend/Controller/Commission/",
      "App\\Backend\\Controller\\Etudiant\\": "src/Backend/Controller/Etudiant/",
      "App\\Backend\\Controller\\PersonnelAdministratif\\": "src/Backend/Controller/PersonnelAdministratif/",
      "App\\Backend\\Controller\\Common\\": "src/Backend/Controller/Common/",
      "App\\Frontend\\View\\": "src/Frontend/views/"
    }
  },
  "scripts": {
    "serve": "php -S localhost:8000 -t Public",
    "post-install-cmd": [
      "@composer dump-autoload"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
